@page
@model MyApp.Namespace.DeleteModel
@{
}

<h1>Eliminar Persona</h1>
<br>

<div>
    <div class="row">
        <dt class="col-md-2">
            <label></label>
            @Html.DisplayNameFor(Model => Model.Personas.Nombres)
           
        </dt>
        
        <dt class="col-md-2">
            @Html.DisplayNameFor(Model => Model.Personas.Identidad)
            
        </dt>
        
        <dt class="col-md-2">
            @Html.DisplayNameFor(Model => Model.Personas.Sexo)
            
        </dt>
        
        <dt class="col-md-2">
            @Html.DisplayNameFor(Model => Model.Personas.Edad)
           
        </dt>
        
    

    </div>
</div>
<form method="post">
    <input type="hidden" asp-for="Personas.Id"/>
    <input type="submit" value="eliminar" class="btn btn-danger"/>
</form>
<br>

<div>
    <a asp-page="./List">Listar Personas</a>
</div>